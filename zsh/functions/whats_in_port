# List process running on provided port
#
# whats_in_port 3000
#
# output:
# COMMAND   PID   USER   FD   TYPE             DEVICE SIZE/OFF NODE NAME
# ruby    25583   root   11u  IPv4 0xee20607697a79bf7      0t0  TCP *:irdmi (LISTEN)

whats_in_port() {
  if [ -n "$1" ]; then
    lsof -ni4TCP:"$1"
  else
    echo >&2 Usage: whats_in_port port-number
    exit 1
  fi
}
